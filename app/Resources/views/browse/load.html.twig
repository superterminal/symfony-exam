{% extends 'base.html.twig' %}

{% block main %}

    Current Page: {{ data.page }} <br>
    Total Pages: {{ data.totalResults }}
    <br>
    {% for movie in data.results %}
        {{ movie.original_title }}<br>
        <img src="{{ loadImage(movie.poster_path) }}" alt="" width="200px" height="300px">
        <hr>
    {% endfor %}

{% endblock %}